@{
    var links = new Dictionary<string, string>();
    links.Add("Home", "Main");
    links.Add("Company", "Companies");
    links.Add("SectorsAndIndustries", "Sectors");
    links.Add("Data", "Data");
    links.Add("RatiosBuilder", "Ratios builder");
    links.Add("Summary", "Summary");
    var active = ViewBag.CurrentPage;
}

<div class="logo">

</div>
<ul class="menu">
    @foreach (var link in links)
    {
        <li class="menu_row menu_row_light @(link.Key == active ? "selected" : "")">
            <i></i>
            @Html.ActionLink(link.Value, "Index", link.Key)
        </li>
    }
</ul>